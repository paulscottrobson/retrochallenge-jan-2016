    1150/     800 :                     ;													Tape Format. 
    1151/     800 :                     ;
    1152/     800 :                     ; ****************************************************************************************************************
    1153/     800 :                     ;
    1154/     800 :                     ;		1 x start bit 		'1' value is held for period of time.
    1155/     800 :                     ;		8 x data bits  		'0 or 1' value is held for a period of time.
    1156/     800 :                     ;		1 x continuation	'0' if another bit follows, '1' if end.
    1157/     800 :                     ;		at least 2 bit times between bytes.
    1158/     800 :                     ;
    1159/     800 :                     ;		Use DLY 4 with A = 0 (DLY 6 to skip half-start)
    1160/     800 :                     ; 		= 13 + 2 * 0 + 514 * 4 microcycles
    1161/     800 :                     ;		= 2,069 microcycles
    1162/     800 :                     ;	
    1163/     800 :                     ;		which is about 240 bits per second.
    1164/     800 :                     ;
    1165/     800 :                     ; ****************************************************************************************************************
    1166/     800 :                     ;
    1167/     800 :                     ;												Monitor Commands
    1168/     800 :                     ;
    1169/     800 :                     ; ****************************************************************************************************************
    1170/     800 :                     ;
    1171/     800 :                     ;		A [aaaa] 			Set current address to aaaa
    1172/     800 :                     ;		B [cc] [dd] [ee]..	Fill memory from current address
    1173/     800 :                     ; 		C 					Clear screen
    1174/     800 :                     ;		D [aaaa] 			Disassemble from aaaa
    1175/     800 :                     ;		G aaaa 				Run from address - address must be given - return with XPPC P3
    1176/     800 :                     ; 		L n 				Set label n to the current address (up to 32 labels 00-1F)
    1177/     800 :                     ; 		M [aaaa] 			Memory dump from current address/aaaa (6 lines, 4 bytes per line)
    1178/     800 :                     ; 		GET [aaaa] 			Load tape to current address/aaa
    1179/     800 :                     ;		PUT [nnnn]			Write nnnn bytes from current address onwards to tape.
    1180/     800 :                     ;
    1181/     800 :                     ;		Command Line Assembler
    1182/     800 :                     ;
    1183/     800 :                     ;		Standard SC/MP mnemonics, except for XPAH, XPAL, XPPC, HALT and DINT which are XPH XPL XPC HLT DIN
    1184/     800 :                     ;		respectively (4 character mnemonics not supported)
    1185/     800 :                     ;
    1186/     800 :                     ;		Address modes are written as such:
